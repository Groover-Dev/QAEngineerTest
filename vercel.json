{
  "version": 2,
  "routes": [
    {
      "src": "/api/photos/(?<id>[^/]+)",
      "dest": "/api/photos/[id].ts?id=$id"
    },
    {
      "src": "/api/curated",
      "dest": "/api/curated.ts"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["api/**"]
      }
    }
  ]
}
