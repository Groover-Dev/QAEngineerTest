{
  "version": 2,
  "routes": [
    {
      "src": "/api/photos/:id",
      "dest": "/api/photos.js"
    },
    {
      "src": "/api/curated",
      "dest": "/api/curated.js"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["api/**"]
      }
    }
  ],
  "build": {
    "env": {
      "NUXT_ENV_PEXELS_API": true
    }
  }
}
